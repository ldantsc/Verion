@page
@using Verion.Areas.VerionAnalysis.Pages.RenderPages;
@model OperationModel
@{
	ViewData["Title"] = "Application";
	Layout = "~/Areas/VerionAnalysis/Pages/_AnalysisLayout.cshtml";
}

<div class="mt-3 mb-3">
	<div id="vehicles-nav" class="bg-white">
		<!-- Map Buttons -->
		<div class="d-flex justify-content-end py-4">
			<button id="message" type="button" class="btn btn-secondary me-2 shadow-sm">Message</button> <!-- Message -->
			<button id="statistics" type="button" class="btn btn-secondary me-2 shadow-sm">Statistics</button> <!-- Statistics-->
			<button id="researches" type="button" class="btn btn-secondary me-2 shadow-sm">Researches</button> <!-- Research -->
			<button id="tracks" type="button" class="btn btn-secondary me-2 shadow-sm">Track</button> <!-- Track -->
			<button id="alarms" type="button" class="btn btn-secondary me-2 shadow-sm">Alarms</button> <!-- Alarms -->
		</div> <!-- Map Buttons /> -->
		<!-- Map Track Vehicles -->
		<div id="map-vehicles" class="mb-2">
			<img src="~/images/map-test.png" width="100%" alt="Minha Figura">
		</div>
	</div>
</div>
